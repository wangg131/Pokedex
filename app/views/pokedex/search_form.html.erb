<h1><%= "Pokedex" %></h1>
<%= form_tag(root_path, method: "get", class: "navbar-form") do |f| %>
    <h2><%= label_tag(:pokemon, "Find your Pokemanz")%></h2><br>
    <%= text_field_tag(:pokemon, params[:id], size: 30, class: "text_box") %>
    <%= submit_tag("Search", class: "search") %>
<% end %>
<div class="results"></div>
